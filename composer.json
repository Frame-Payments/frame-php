{
    "name": "framepayments/php-sdk",
    "require": {
      "php": "^8.1",
      "friendsofphp/php-cs-fixer": "^3.0",
      "guzzlehttp/guzzle": "^7.0",
      "league/iso3166": "^4.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "mockery/mockery": "^1.6"
    },
    "autoload": {
      "psr-4": {
        "Frame\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Frame\\Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "phpunit",
      "test-coverage": "phpunit --coverage-html coverage"
    }
  }
